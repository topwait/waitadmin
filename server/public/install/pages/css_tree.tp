/** 小的屏幕 **/
@media screen and (max-width: 768px){
    /*主框架部分*/
    .wait-header .layui-layout-left {left: 0;}
    .wait-body {left: 0 !important;}
    .wait-tabs {left: 0 !important;}
    .wait-sidebar {width: 200px; transform: translate3d(-200px, 0, 0); -webkit-transform: translate3d(-200px, 0, 0);}
    .wait-sidebar.develop-sidebar {transform: translate3d(0, 0, 0); -webkit-transform: translate3d(0, 0, 0);}

    /*layui部分*/
    .layui-form-item .layui-input-inline { margin-left: 0; }
    .layui-card .layui-card-header { padding: 5px; }
    .layui-tab-title.layui-tab-more { z-index: 1000; background: #FFF; }
}

/** 首页加载动画 **/
@keyframes wait-anim-loader {
    17% { border-bottom-right-radius: 3px; }
    25% { transform: translateY(9px) rotate(22.5deg); }
    50% { transform: translateY(18px) scale(1, 0.9) rotate(45deg); border-bottom-right-radius: 40px;}
    75% { transform: translateY(9px) rotate(67.5deg); }
    100% { transform: translateY(0) rotate(90deg); }
}

@-webkit-keyframes wait-anim-loader {
    17% {border-bottom-right-radius: 3px;}
    25% {transform: translateY(9px) rotate(22.5deg);}
    50% {transform: translateY(18px) scale(1, 0.9) rotate(45deg); border-bottom-right-radius: 40px;}
    75% {transform: translateY(9px) rotate(67.5deg);}
    100% {transform: translateY(0) rotate(90deg);}
}

@keyframes wait-anim-shadow {
    0%, 100% {transform: scale(1, 1);}
    50% {transform: scale(1.2, 1);}
}

@-webkit-keyframes wait-anim-shadow {
    0%, 100% {transform: scale(1, 1);}
    50% {transform: scale(1.2, 1);}
}


/** 过场加载动画 **/
@-webkit-keyframes ball-loader-load {
    0% {transform:scale(0); -webkit-transform:scale(0);}
    50% {transform:scale(1); -webkit-transform:scale(1);}
    100% {transform:scale(0); -webkit-transform:scale(0);}
}

@keyframes ball-loader-load {
    0% {transform:scale(0); -webkit-transform:scale(0);}
    50% {transform:scale(1); -webkit-transform:scale(1);}
    100% {transform:scale(0); -webkit-transform:scale(0);}
}

@keyframes ball-loader-close {
    0% {opacity: 1;}
    100% {opacity: 0;}
}

/** Layui扩展动画 **/
.layui-anim-left {-webkit-animation-name: wait-anim-left; animation-name: wait-anim-left;}
.layui-anim-right {-webkit-animation-name: wait-anim-right; animation-name: wait-anim-right;}
.layui-layer-left {top: 50px !important; bottom: 0; box-shadow: 1px 1px 10px rgba(0,0,0,.1); border-radius: 0; overflow: auto;}
.layui-layer-right {top: 50px !important; bottom: 0; box-shadow: 1px 1px 10px rgba(0,0,0,.1); border-radius: 0; overflow: auto;}
@-webkit-keyframes wait-anim-left {
    from {
        -webkit-transform: translate3d(-100%, 0, 0);
        transform: translate3d(-100%, 0, 0);
        visibility: visible;
    }

    to {
        -webkit-transform: translate3d(0, 0, 0);
        transform: translate3d(0, 0, 0);
    }
}

@keyframes wait-anim-left {
    from {
        -webkit-transform: translate3d(-100%, 0, 0);
        transform: translate3d(-100%, 0, 0);
        visibility: visible;
    }

    to {
        -webkit-transform: translate3d(0, 0, 0);
        transform: translate3d(0, 0, 0);
    }
}

@-webkit-keyframes wait-anim-right {
    from {
        -webkit-transform: translate3d(100%, 0, 0);
        transform: translate3d(100%, 0, 0);
        visibility: visible;
    }

    to {
        -webkit-transform: translate3d(0, 0, 0);
        transform: translate3d(0, 0, 0);
    }
}

@keyframes wait-anim-right {
    from {
        -webkit-transform: translate3d(100%, 0, 0);
        transform: translate3d(100%, 0, 0);
        visibility: visible;
    }

    to {
        -webkit-transform: translate3d(0, 0, 0);
        transform: translate3d(0, 0, 0);
    }
}
