/** 全局样式 **/
html,body {margin: 0; padding: 0; font-size: 12px; background-color: #f5f7f9;}
body,a,dl,dt,dd,li,img {cursor: url('../images/default/cursor.cur'), auto !important;}
::-webkit-input-placeholder{color: #ccc;}
.layui-layer-adminRight{top: 50px !important; bottom: 0; box-shadow: 1px 1px 10px rgba(0,0,0,.1); border-radius: 0; overflow: auto;}

/** 头部样式 **/
.wait-header {position: fixed; top: 0; left: 0; z-index: 1000; width: 100%; height: 50px; box-sizing: border-box; border-bottom: 1px solid #f6f6f6; background-color: #fff;}
.wait-header a,
.wait-header a cite {color: #333 !important;}
.wait-header a:hover {color: #000 !important;}
.wait-header .layui-nav {padding-right: 0; background: 0 0;}
.wait-header .layui-nav .layui-nav-item {height: 50px; line-height: 50px;}
.wait-header .layui-nav .layui-nav-item .layui-icon{ position: relative; top: 1px; font-size: 16px;}
.wait-header .layui-layout-left {padding: 0 10px; left: 200px}
.wait-header .layui-layout-left a {padding: 0;}
.wait-header .layui-layout-left .layui-nav-item {margin: 0 20px;}
.wait-header .layui-layout-right .layui-nav-more {border-top-color: #666;}
.wait-header .layui-layout-right .layui-nav-mored {border-color: transparent; border-bottom-color: #666;}
.wait-header .layui-layout-right .user-info.layui-nav-child { top: 50px !important;}
.wait-header .layui-nav .layui-this:after,
.wait-header .layui-nav-bar {top: 0 !important; bottom: auto; height: 3px; background-color: #fff; background-color: rgba(255,255,255,.3);}
.wait-header .layui-nav-bar {height: 2px; background-color: #20222a;}

/** 菜单样式 **/
.wait-sidebar::-webkit-scrollbar {width: 0;}
.wait-sidebar {position: absolute; top: 0; left: 0; bottom: 0; z-index: 3000; width: 200px; overflow: auto; overflow-x: hidden; background-color: #20222a;}
.wait-sidebar ul {margin-top: 50px;}
.wait-sidebar .logo {position: fixed; z-index: 2100; width: 200px; height: 50px; line-height: 50px; text-align: center; box-shadow: 0 1px 1px 0 rgba(0,0,0,0.15); background: #20222a url(../images/side_lg_logo.png) no-repeat center;}
.wait-sidebar .logo img {max-width: 200px; max-height: 200px; vertical-align: middle;}
.wait-sidebar .logo span {color: #fff; font-size: 18px;}
.wait-sidebar .wait-menu-item a {position: relative; display: block; overflow: hidden; color: #bababc; font-size: 14px; height: 40px; line-height: 40px; padding: 6px 13px 6px 38px; border-left: 4px solid transparent;}
.wait-sidebar .wait-menu-item a i {position: absolute; top: 50%; left: 15px; margin-top: -20px; font-size: 16px;}
.wait-sidebar .wait-menu-item a span {float: right; font-size: 14px;}
.wait-sidebar .wait-menu-item a cite {font-size: 14px; overflow:hidden; white-space: nowrap; text-overflow: ellipsis;}
.wait-sidebar .wait-menu-item.on > a {color: #fff;}
.wait-sidebar .wait-menu-item.on {background-color: #009688;}
.wait-sidebar .wait-menu-item:hover > a {color: #fff; border-left: #009688 4px solid;}
.wait-sidebar .wait-menu-item.active > a {color: #fff;}
.wait-sidebar .wait-menu-item .wait-second-menu {display: none; padding: 5px 0; background-color: #16181d;}
.wait-sidebar .wait-menu-item .wait-second-menu dd dl {padding: 5px 0;}
.wait-sidebar .wait-menu-item .wait-second-menu dd:hover  > a {color: #fff;}
.wait-sidebar .wait-menu-item .wait-second-menu dd.active > a {color: #fff;}
.wait-sidebar .wait-menu-item .wait-second-menu dd a i {float: none; padding: 0;}
.wait-sidebar .wait-menu-item .wait-second-menu dd.on > a {color: #fff; }
.wait-sidebar .wait-menu-item .wait-second-menu dd.on {background-color: #009688;}
.wait-sidebar .wait-menu-item .wait-second-menu dd a {padding: 0 0 0 34px;}
.wait-sidebar .wait-menu-item .wait-second-menu dd span {padding-right: 13px;}
.wait-sidebar .wait-menu-item .wait-second-menu dl {display: none;}
.wait-sidebar .wait-menu-item .wait-second-menu dl dd a {padding-left: 48px;}
.wait-sidebar .wait-menu-item .wait-second-menu dl dd a:hover {color: #ffffff;}

/* 收缩菜单 */
.wait-side-shrink .wait-sidebar .logo {width: 60px; background: #20222a url(../images/side_xs_logo.png) no-repeat center;}
.wait-side-shrink .wait-sidebar .logo img {display: none;}
.wait-side-shrink .wait-sidebar .logo span {display: none;}
.wait-side-shrink .wait-header .layui-layout-left {left: 60px;}
.wait-side-shrink .wait-sidebar { width: 60px; }
.wait-side-shrink .wait-sidebar .wait-menu-item > a {padding-right: 0;}
.wait-side-shrink .wait-sidebar .wait-menu-item > a cite {display: none;}
.wait-side-shrink .wait-sidebar .wait-menu-item > a span {display: none;}
.wait-side-shrink .wait-sidebar .wait-menu-item.active {background: rgba(0,0,0,.3);}
.wait-side-shrink .wait-tabs {left: 60px;}
.wait-side-shrink .wait-body {left: 60px;}

/** 页面标签 **/
.wait-tabs {position: fixed; top: 50px; left: 200px; right: 0; z-index: 999;}
.wait-tabs {height: 40px; line-height: 40px; padding: 0 80px 0 40px; background-color: #fff; box-sizing: border-box; box-shadow: 0 1px 1px 0 rgba(0,0,0,.05);}
.wait-tabs .lay-tabs-control {position: absolute; top: 0; width: 40px; height: 100%; text-align: center; cursor: pointer; transition: all .3s; -webkit-transition: all .3s; box-sizing: border-box; border-left: 1px solid #f6f6f6;}
.wait-tabs .lay-tabs-control:hover {background-color: #f6f6f6;}
.wait-tabs .layui-icon-prev {left: 0; border-left: none; border-right: 1px solid #f6f6f6;}
.wait-tabs .layui-icon-next {right: 40px;}
.wait-tabs .layui-icon-down {right: 0;}
.wait-tabs .lay-tabs-select.layui-nav {position: absolute; left: 0; top: 0; width: 100%; height: 100%; padding: 0; background: none;}
.wait-tabs .lay-tabs-select.layui-nav .layui-nav-item {line-height: 40px;}
.wait-tabs .lay-tabs-select.layui-nav .layui-nav-item > a {height: 40px;}
.wait-tabs .lay-tabs-select.layui-nav .layui-nav-item a {color: #666;}
.wait-tabs .lay-tabs-select.layui-nav .layui-nav-child {top: 40px; left: auto; right: 0;}
.wait-tabs .lay-tabs-select.layui-nav .layui-nav-child dd.layui-this,
.wait-tabs .lay-tabs-select.layui-nav .layui-nav-child dd.layui-this a {background-color: #f2f2f2!important; color: #333;}
.wait-tabs .lay-tabs-select.layui-nav .layui-nav-more,
.wait-tabs .lay-tabs-select.layui-nav .layui-nav-bar {display: none; }
.wait-tabs .layui-tab {margin: 0; overflow: hidden;}
.wait-tabs .layui-tab-title {height: 40px; border: none; color: #666;}
.wait-tabs .layui-tab-title li {min-width: 0; line-height: 40px; max-width: 160px; text-overflow: ellipsis; padding-right: 40px; overflow: hidden; border-right: 1px solid #f6f6f6; vertical-align: top;}
.wait-tabs .layui-tab-title li:first-child { padding-right: 15px;}
.wait-tabs .layui-tab-title li:first-child .layui-tab-close { display: none; }
.wait-tabs .layui-tab-title li .layui-tab-close {position: absolute; right: 8px; top: 50%; margin: -7px 0 0 0; width: 16px; height: 16px; line-height: 16px; border-radius: 50%; font-size: 12px;}
.wait-tabs .layui-tab-title li:after {content:''; position: absolute; top: 0; left: 0; width: 0; height: 2px; border-radius: 0; background-color: #EBF4FF; transition: all .3s; -webkit-transition: all .3s;}
.wait-tabs .layui-tab-title li:hover:after {width: 100%;}
.wait-tabs ul.layui-tab-title li:hover,
.wait-tabs ul.layui-tab-title li.layui-this{background-color: #f6f6f6;}
.wait-tabs ul.layui-tab-title li.layui-this:after {width: 100%; border: none; height: 2px; background-color: #20222a !important;}
.wait-tabs ul.layui-tab-title .layui-this { color: #000 !important; }
.wait-tabs ul.layui-tab-title li:after {background-color: #20222a !important;}
.wait-tabs .lay-tabs-select.layui-nav .layui-nav-child dd.layui-this a { color: #666; background-color: #fff !important;}

/** 主体样式 **/
.wait-body {position: fixed; top: 90px; left: 200px; right: 0; bottom: 0; z-index: 900; min-width: 300px;}
.wait-body .tab-body-item {display: none; width: 100%; height: 100%;}
.wait-body .tab-body-item iframe {width: 100%; height: 100%; border: 0;}

/** 遮罩样式 **/
.wait-mask {position: fixed; left: 0; right: 0; top: 0; bottom: 0; z-index: 2000; display: none; background-color: rgba(0,0,0,.3); }
.wait-mask.activate {display: block;}

/** 首页动画 **/
.wait-load {position: fixed; z-index: 9999999; width: 100%; height: 100%; background-color: whitesmoke;}
.wait-load .loader {position: relative; z-index: 999999; width: 50px; height: 50px; margin: 20% auto 40px; background-color: whitesmoke;}
.wait-load .loader:before {position: absolute; top: 59px; left: 0; opacity: 0.1; content: ""; width: 50px; height: 7px; border-radius: 50%; background: #000; animation: wait-anim-shadow .5s linear infinite;}
.wait-load .loader:after {position: absolute; top: 0; left: 0; content: ""; width: 50px; height: 50px; border-radius: 3px; background-color: #5FB878; animation: wait-anim-loader .5s linear infinite;}

/** 过场动画 **/
.wait-loading {position: absolute; display: block; width: 100%; height: 100%; top: 0; left: 0; right: 0; bottom: 0; z-index: 1980000; background-color: #f5f7f9;}
.wait-loading .close {animation: close 1s; -webkit-animation: ball-loader-close 1s; animation-fill-mode: forwards;}
.wait-loading .ball-loader {position: absolute; left: 50%; top: 50%; transform: translate(-50%, -50%);}
.wait-loading .ball-loader>span {display: inline-block; background-color: #4aca85;}
.wait-loading .ball-loader>span:nth-child(2) {-webkit-animation-delay: .1s; animation-delay: .1s;}
.wait-loading .ball-loader>span:nth-child(3) {-webkit-animation-delay: .15s; animation-delay: .15s;}
.wait-loading .ball-loader>span:nth-child(4) {-webkit-animation-delay: .2s; animation-delay: .2s;}
.wait-loading .ball-loader>span {width: 20px; height: 20px; margin: 0 3px; border-radius: 50%; transform: scale(0); -ms-transform: scale(0); -webkit-transform: scale(0); animation: ball-loader-load 1s ease-in-out infinite;  -webkit-animation:1s ball-loader-load ease-in-out infinite;}

/** 过度动画 **/
.wait-tabs,
.wait-body,
.wait-header,
.wait-sidebar,
.wait-sidebar .logo,
.wait-header .layui-layout-left {transition: all .3s; -webkit-transition: all .3s;}

/** 隐藏标签 **/
body[data-tab="true"] .wait-tabs {display: none;}
body[data-tab="true"] .wait-body {top: 50px;}

